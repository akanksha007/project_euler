#Akanksha Agarwal


for i in range (1,500):
    a=[]
    b=[]
    k=(i*(i+1))/2
    a.append(1)
    b.append(k)
    for j in range (2,10):
        t=k%j
        if(t==0):
            a.append(j)
            x=k/j
            if(x>=10):
                b.append(x)
        
    maxa=max(a)
    
    minb=min(b)
    
    if(maxa!=1 or minb!=k):
        for x in range(maxa,minb):
            t=k%x
            if(t==0):
                a.append(x)
    flag=len(a)+len(b)
    
    if (flag>100):
        print"the first no is the triangular series which has factor more than 100 is \n",k 
        break;         
    
    
        
        
